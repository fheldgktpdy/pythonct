    "#count 함수!!\n",
    "\n",
    "t = int(input())\n",
    "for i in range(t):\n",
    "    answer = 0\n",
    "    n,m = map(int,input().split())\n",
    "    for j in range(n,m+1):\n",
    "        for k in range(len(str(j))):\n",
    "            if str(j)[k]=='0':\n",
    "                answer+=1\n",
    "    print(answer)"
   ]
