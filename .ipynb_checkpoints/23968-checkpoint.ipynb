    "li = list(map(int,input().split()))\n",
    "sum=0\n",
    "num = len(li)-1\n",
    "for j in range(len(li)):\n",
    "    for i in range(num):\n",
    "        if li[i] > li[i+1]:\n",
    "            li[i],li[i+1] = li[i+1],li[i]\n",
    "            sum+=1\n",
    "    num-=1\n",
    "if sum<k:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(sum)"
