    "# 이중 리스트에 담은뒤, 횟수세주기\n",
    "N = int(input())\n",
    "lis = []\n",
    "num = 0\n",
    "for i in range(N):\n",
    "    lis.append(list(map(int,input().split())))\n",
    "for i in range(len(lis)-1):\n",
    "    for j in range(i+1,len(lis)):\n",
    "    if lis[i][0]==lis[j][0]:\n",
    "        if lis[i][1]!=lis[j][1]:\n",
    "            num+=1\n",
    "        del lis[j]"
